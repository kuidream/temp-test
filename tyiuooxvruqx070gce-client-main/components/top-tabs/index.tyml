<view class="top-tabs">
  <view class="tab-item {{current === 'store' ? 'active' : ''}}" data-tab="store" bindtap="onTap">
    <text class="tab-text">{{i18n.t('tab_store')}}</text>
  </view>
  <view class="tab-item {{current === 'device' ? 'active' : ''}}" data-tab="device" bindtap="onTap">
    <text class="tab-text">{{i18n.t('tab_device')}}</text>
  </view>
</view>
